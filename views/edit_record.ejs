
<%- contentFor('content') %>
<div class="right_col" role="main">
          
          <!-- top tiles -->
          <div class="panel panel-primary" style="width:50%;margin:0 auto; margin-top:10%; margin-bottom:10%">
<div class="panel-heading"><h3>Edit Amazon MWS API's</h3></div>
<div class="panel-body" style="height:40%; text-align:center; margin:10px;">
<p class="bg-info" id="msg"></p>
 <form class="form-horizontal" role="form" id="" method="post" action="/update?id=<%= user._id %>">


 
  <div class="form-group">
    <label class="control-label col-sm-5 setAlign" for="marketplaceid">MarketPlaceId:</label>
     <div class="input-group date col-sm-7">
        <input type="text" class="form-control" name="marketplaceid" value="<%= user.mws[0].marketplaceid %>" />
     </div>
  </div>
 
  
  <div class="form-group">
    <label class="control-label col-sm-5 setAlign" for="secretkey">Secret Key:</label>
     <div class="input-group date col-sm-7">
        <input type="text" class="form-control" value="<%= user.mws[0].secretkey %>" name="secretkey" />
     </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-5 setAlign" for="awsaccesskeyid">AWSAccessKeyId:</label>
     <div class="input-group date col-sm-7">
        <input type="text" class="form-control" value="<%= user.mws[0].awsaccesskeyid %>" name="awsaccesskeyid" />
     </div>
  </div>

  
 
  <div class="form-group">
    <div class="col-sm-12">
      <button id="send" class="btn btn-primary btn-lg" type="submit">
         <span class="glyphicon glyphicon-send" ></span> Submit
      </button>
    </div>
  </div>
</form>
</div>
</div>
 

  
</div>



































